                    <script type="text/javascript">
                        var scrollPage = 1;
                        $(".scrollable").scrollable();
                        var api = $(".scrollable").data("scrollable");

                        api.onSeek(function() {
                            if (api.getIndex() + 1 > scrollPage) {
                                GoNext($(".next"));
                            } else {
                                DoPrev();
                            }
                        });

                        function SetScrollPage() {
                            $('.navi').html('Page ' + scrollPage);
                        }

                        function DoPrev() {
                            if (scrollPage > 1) {
                                scrollPage -= 1;
                                SetScrollPage();
                            }
                        }

                        function GoNext(nextVar) {
                            var nextEle = nextVar;
                            nextEle.hide();

                            if (scrollPage < api.getSize()) {
                                scrollPage += 1;
                                SetScrollPage();
                            }

                            var aCurrPage = $('.items div > a img[page=' + (scrollPage + 1) + ']');
                            aCurrPage.each(function() {
                                $(this).attr('src', $(this).attr('srcTemp'));
                            });

                            if (api.getSize() == scrollPage) {
                                var numItems = $('.items div > a').size();
                                $.ajax({
                                    url: '/Home/GetNextUpdatedComic',
                                    type: "POST",
                                    dataType: "html",
                                    data: { id: numItems, page: scrollPage },
                                    beforeSend: function() {
                                        $(".ajxloading").css({ "bottom": "0" }).show();
                                    },
                                    success: function(dataResponse, textStatus) {
                                        if (dataResponse != "") {
                                            api.addItem(dataResponse);
                                            if (api.getSize() == api.getIndex() + 1) // last div
                                                api.next();

                                            var aCurrPage = $('.items div > a img[page=' + (scrollPage + 1) + ']');
                                            aCurrPage.each(function() {
                                                $(this).attr('src', $(this).attr('srcTemp'));
                                            });
                                        }

                                        setTimeout(function() {
                                            nextEle.show();
                                        }, 2000);
                                    },
                                    error: function(XMLHttpRequest, textStatus, errorThrown) {

                                    },
                                    complete: function(XMLHttpRequest, textStatus) {

                                    }
                                }); // end ajax
                            }
                            else {
                                nextEle.show();
                            }
                        }
                    </script>
